{
  "name": "pavlusha311245/oo-program",
  "description": "This is a simple program that reads a CSV file and outputs the data in a table format.",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Zavadski",
      "email": "pavel.zavadski@pavlusha.me"
    }
  ],
  "scripts": {
    "lint": [
      "./vendor/bin/pint"
    ],
    "test": [
      "./vendor/bin/pest"
    ]
  },
  "require": {
    "php": "^8.3",
    "illuminate/database": "^11.5",
    "illuminate/validation": "^11.44",
    "laravel-zero/framework": "^11.36.1",
    "maatwebsite/excel": "^3.1"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "laravel/pint": "^1.18.3",
    "mockery/mockery": "^1.6.12",
    "pestphp/pest": "^3.7.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "bin": [
    "artisan"
  ]
}
